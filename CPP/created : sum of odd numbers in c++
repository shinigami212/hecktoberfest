#include <iostream>
using namespace std;

int solve( int n ) {
   int i;
   int sum = 0;
   cout << "Odd numbers are: ";
   for( i = 1; i <= n; i++ ) {
      if( i % 2 == 1 ) {
         cout << i << ", ";
         sum = sum + i;
      }
   }

   cout << endl;
   return sum;
}
int main(){
   int sum = solve( 25 );
   cout << "Sum is: " << sum;
}
